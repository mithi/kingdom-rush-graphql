    ],
    "author": "Mithi",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/mithi/kingdom-rush-graphql/issues"
    },
    "homepage": "https://github.com/mithi/kingdom-rush-graphql#readme",
    "devDependencies": {
        "@types/graphql": "^14.5.0",
        "@types/jest": "^26.0.14",
        "jest": "^26.5.2",
        "nodemon": "^2.0.5",
        "prettier": "^2.1.2",
        "ts-jest": "^26.4.1",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.3"
    },
    "dependencies": {
        "@types/express": "^4.17.8",
        "apollo-server": "^2.18.2",
